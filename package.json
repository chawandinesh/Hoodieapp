{
  "name": "Hoodie",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint *.js **/*.js",
    "flow": "flow",
    "flow-stop": "flow stop",
    "rename": "react-native-rename Student Manager -b org.studentmanager",
    "pretty": "prettier --semi false --print-width 100 --single-quote --trailing-comma all --write  \"src/**/*.js\"",
    "build": "cd android && ./gradlew assembleRelease",
    "testbuild": "react-native run-android --variant=release",
    "cleanup": "cd android && ./gradlew clean",
    "jetify": "npx jetify",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "del-ios": "cd android/app/src/main/res && rm -rf raw drawable-hdpi drawable-mdpi drawable-xhdpi drawable-xxhdpi drawable-xxxhdpi",
    "del-android": "cd android/app/src/main/res && del raw drawable-hdpi drawable-mdpi drawable-xhdpi drawable-xxhdpi drawable-xxxhdpi",
    "build-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/netinfo": "^5.9.7",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "firebase": "^7.19.1",
    "i18n-js": "^3.5.1",
    "moment": "^2.22.2",
    "native-base": "^2.13.8",
    "prop-types": "^15.6.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-animatable": "^1.3.3",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-modalbox": "^2.0.0",
    "react-native-onesignal": "^3.9.2",
    "react-native-render-html": "^4.2.0",
    "react-native-screens": "^2.0.0-alpha.32",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^2.14.2",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^10.10.0",
    "react-navigation": "^3.11.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^25.5.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-flowtype": "^3.7.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^3.0.1",
    "jest": "^25.5.0",
    "jetifier": "^1.6.3",
    "lint-staged": "^9.2.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "react-test-renderer": "16.11.0",
    "reactotron-react-native": "^3.6.4",
    "reactotron-redux": "^3.1.1",
    "reactotron-redux-saga": "^4.2.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
